{
  "name": "pollen-solutions/wp-term",
  "description": "Pollen Solutions - Wordpress Term Component - Abstraction layer of WP_Term object.",
  "homepage": "https://www.presstify.com/pollen-solutions/wp-term/",
  "keywords": [
    "pollen-solutions",
    "component",
    "wordpress",
    "taxonomy",
    "term"
  ],
  "time": "2021-08-10",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "pollen-solutions/support": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Pollen\\WpTerm\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\WpTerm\\Test\\": "tests"
    }
  },
  "suggest": {
    "pollen-solutions/container" : "Pollen Solutions - Container Component - PSR-11 ready Dependencies Injection Container."
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
